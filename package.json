{
  "name": "basicdesktopapp",
  "version": "2.0.0",
  "description": "A Desktop App using the AppAuth-JS security library",
  "main": "main.bundle.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' 'scripts/*.ts'",
    "buildnative": "node_modules/.bin/electron-rebuild",
    "build": "npm run clean && npm run buildMainDev && npm run buildRendererDev",
    "clean": "rimraf dist && copyfiles index.html desktop*.json css/* images/* package.json dist",
    "buildMainDev": "webpack --config webpack/webpack.config.main.dev.js && copyfiles -u 1 src/preload.js dist",
    "buildMainProd": "webpack --config webpack/webpack.config.main.prod.js && copyfiles -u 1 src/preload.js dist",
    "buildRendererDev": "webpack --config webpack/webpack.config.renderer.dev.js",
    "buildRendererProd": "webpack --config webpack/webpack.config.renderer.prod.js",
    "start": "node_modules/.bin/electron ./dist",
    "buildRelease": "npm run clean && npm run buildMainProd && npm run buildRendererProd",
    "pack": "npm run buildRelease && rimraf package && electron-packager ./dist --out=package"
  },
  "build": {
    "protocols": {
      "name": "basicdesktopapp",
      "schemes": [
        "x-mycompany-desktopapp"
      ]
    }
  },
  "files": [
    "dist/**"
  ],
  "keywords": [],
  "author": "Gary Archer",
  "license": "ISC",
  "dependencies": {
    "@openid/appauth": "^1.2.7",
    "axios": "^0.19.2",
    "electron-default-menu": "^1.0.1",
    "electron-log": "^4.0.7",
    "fs-extra": "^8.1.0",
    "guid-typescript": "^1.0.9",
    "keytar": "^5.2.0",
    "moment": "2.24.0",
    "opener": "^1.5.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-modal": "^3.11.1",
    "react-router-dom": "^5.1.2",
    "url-parse": "^1.4.7",
    "username": "^4.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-modal": "^3.10.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/url-parse": "^1.4.3",
    "copyfiles": "^2.3.0",
    "electron": "9.0.0",
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^1.10.0",
    "node-loader": "^0.6.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.2",
    "tslint-react": "^4.1.0",
    "typescript": "^3.9.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2"
  }
}
